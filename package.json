{
  "name": "commentcleaner",
  "displayName": "commentcleaner",
  "description": "VS Code Extension to clean up comments in your code",
  "version": "0.0.1",
  "publisher": "mihirpanchal",
  "icon": "assets/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/MihirRajeshPanchal/commentcleaner"
  },
  "keywords": [
    "comment",
    "comments",
    "clean",
    "purge",
    "clean comments",
    "commentcleaner",
    "panchal",
    "multilanguage",
    "cleancomment",
    "remove comments",
    "remove",
    "mihir"
  ],
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Formatters","Debuggers"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.removeComments",
        "title": "CommentCleaner",
        "icon": "$(clear-all)"
      }
    ],
    "keybindings": [
      {
        "command": "extension.removeComments",
        "key": "ctrl+'",
        "mac": "cmd+'",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "extension.removeComments",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
